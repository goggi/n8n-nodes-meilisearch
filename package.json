{
  "name": "n8n-nodes-meilisearch-v2",
  "version": "0.1.3",
  "description": "A community node to interact with the Meilisearch REST API. Minor updates from original version",
  "keywords": [
    "n8n-community-node-package",
    "meilisearch"
  ],
  "license": "MIT",
  "homepage": "https://github.com/goggi",
  "author": {
    "name": "Goggi",
    "email": "gogsaan@gmai.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goggi/n8n-nodes-meilisearch.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MeilisearchApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Meilisearch/Meilisearch.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-core": "^1.6.0",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "~4.8.4"
  }
}
